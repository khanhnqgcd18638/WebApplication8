@model WebApplication8.Models.Trainer
@{
    ViewBag.Title = "DetailsTrainer";
}

<h2>DetailsTrainer</h2>
@Html.ActionLink("Back", "TrainerProfile", "Staff", null, new { @class = "btn btn-primary" })

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">@Model.UserName</h3>
    </div>
    <div class="panel-body">
        @Model.Email
    </div>
    <div class="panel-body">
        @Model.Education
    </div>
    <div class="panel-body">
        @Model.WorkPlace
    </div>
    <div class="panel-body">
        @Model.Telephone
    </div>
    <div class="panel-body">
        @Model.Type
    </div>
</div>



